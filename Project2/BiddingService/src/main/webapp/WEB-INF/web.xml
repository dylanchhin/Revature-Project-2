<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Bidding</servlet-name>
    <servlet-class>servlets.BiddingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuyHistory</servlet-name>
    <servlet-class>servlets.BuyHistoryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CurrentBid</servlet-name>
    <servlet-class>servlets.CurrentBidServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AuctionWinner</servlet-name>
    <servlet-class>servlets.AuctionWinnerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bidding</servlet-name>
    <url-pattern>/bidding</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuyHistory</servlet-name>
    <url-pattern>/buyhistory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CurrentBid</servlet-name>
    <url-pattern>/currentbid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AuctionWinner</servlet-name>
    <url-pattern>/auctionwinner</url-pattern>
  </servlet-mapping>
</web-app>
